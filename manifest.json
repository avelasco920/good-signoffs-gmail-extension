{
  "manifest_version": 2,
  "name": "Good Sign-Offs Gmail Extension",
  "version": "1.2",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "app.js"]
    }
  ],
  "icons": {"128": "Thumb.png"}
}
